{
  "name": "Amin's Adblocker",
  "version": "1.0",
  "description": "Simple ad blocker made by Amin",
  "manifest_version": 3,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "*://*.zedo.com/*"
  ],
  "background": {
    "service_worker": "adblock_1.js"
  },
  "icons": {
    "16": "icons/logo_16x16.png",
    "48": "icons/logo_48x48.png",
    "128": "icons/logo_128x128.png"
  }
}
